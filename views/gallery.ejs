<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="../styles.css">
    <title><%= title %></title>
</head>
<body>
    <head>
        <div id="navbar">
            <div id="nav-logo"><%= title %></div>
            <div id="navbar-trip">

                <% if (imagesFinal.length > 0) { %>
                    <% imagesFinal.forEach(tab => { %>
                        <div class="trip-item"><a href="/<%=tab.name %>"><%= tab.name %></a></div>
                    <% }) %>
                <% } %>

            </div>
        </div>
    </head>
    <main>
        <div id="gallery">

            <% if (gallery.length > 0) { %>
                <% gallery.forEach(image => { %>
                    <%- image %>
                <% }) %>
            <% } %>

        </div>
    </main>
</body>
</html>